.ll-switch {
  height: 24px;
  line-height: 24px;
  color: #00c1df;
  position: relative; }
  .ll-switch input {
    display: none; }
    .ll-switch input:checked + label {
      background: #79edff; }
      .ll-switch input:checked + label:after {
        background: #00c1df;
        left: calc(100% - 24px);
        right: 0; }
  .ll-switch label {
    display: block;
    position: relative;
    width: 50px;
    height: 24px;
    border-radius: 24px;
    -webkit-transition: all 0.3s ease;
    -ms-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    transition: all 0.3s ease;
    background: #d5d5d5;
    cursor: pointer; }
    .ll-switch label:after {
      width: 24px;
      height: 24px;
      display: block;
      content: "";
      position: absolute;
      background: #888;
      left: 0;
      top: 0;
      border-radius: 100%;
      -webkit-transition: all 0.3s ease;
      -ms-transition: all 0.3s ease;
      -moz-transition: all 0.3s ease;
      transition: all 0.3s ease; }

