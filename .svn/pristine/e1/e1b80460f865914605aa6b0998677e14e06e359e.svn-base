@charset "utf-8";
@import"core/setting";
@import"core/css3";
@import"core/_mixin";
html,body{background-color: #f5f6f6}
%iconfont {
  font-family:"iconfont" !important;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  @extend %user-select-none;
}
.main_top{
	position: relative;
	width: 100%;
	height: 287px;
	&:after{
		content: "";
		display: block;
		position: absolute;
		top: 0;
		z-index: 1;
		height: 100%;
		width: 50%;
		left: 0;
		background-color: #313f52;
	}
	&:before{
		content: "";
		display: block;
		position: absolute;
		top: 0;
		z-index: 1;
		height: 100%;
		width: 50%;
		right: 0;
		background-color: #2fcfc5;
	}
	.main_main{
		position: relative;
		z-index: 2;
		margin: 0 auto;
		width: 1200px;
		height: 100%;
		background-color: #ccc;
		&:after{
			content: "";
			display: block;
			position: absolute;
			top: 0;
			z-index: 1;
			height: 100%;
			width: 735px;
			right: 50%;
			background: url("../img/setmeal/setmeal_left.png") no-repeat 0px 0px #000;
			background-size: cover;
		}
		&:before{
			content: "";
			display: block;
			position: absolute;
			top: 0;
			z-index: 1;
			height: 100%;
			width: 735px;
			left: 50%;
			background: url("../img/setmeal/setmeal_right.png") no-repeat 0px 0px;
			background-size: cover;
		}
		>.package{
			position: relative;
			margin-top: 33px;
			text-align: left;
			z-index: 2;
			float: left;
			width: 810px;
			padding: 15px 20px;
			height: 190px;
			background-color: #fff;
			@extend %clearfix;
			>a{
				margin: 25px 0 0 130px; 
				display: block;
				width: 160px;
				height: 40px;
				border-radius: 3px;
				background-color: #ea544a;
				text-align: center;
				line-height: 40px;
				color: #fff;
				font-size: 16px;
				letter-spacing: 1px;
				@include transition;
				&:hover{
					background-color: #f53427;
					box-shadow:  0 0 20px rgba(0, 0, 0, 0.2);
				}
			}
		}
	}
}
.package_ul{
				position: relative;
				@extend %clearfix;
				li{
					position: relative;
					float: left;
					width: auto;
					height: 70px;
					padding: 0 30px 0 15px;
					@extend %clearfix;
					text-align: center;
					>span{
						display: block;
						&:nth-child(1){
							font-size: 14px;
							color: #333;
							font-weight: 700;
						}
						&:nth-child(2){
