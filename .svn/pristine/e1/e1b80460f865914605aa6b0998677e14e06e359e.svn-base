.pn-slide{
	display: none;
	width:450px;
	height: 10px;
	position: relative;
	float: left;
	margin-top:10px;
	@extend %user-select-none;
	.slide-back{
		z-index: 1;
		background: #d1e5e7;
	}
	.slide-front{
		z-index: 2;
		background: #ff4100;
		color:#fff;
		>.container{
			>.item{
				&:after{
					background: #fff;
				}
			}
		}
	}
	.slide-back,.slide-front{
		position: absolute;
		width:100%;
		height: 100%;
		overflow: hidden;
		top:0;
		left: 0;
		>.container{
			width:500px;
			@extend %clearfix;
			>.item{
				width:125px;
				float: left;
				height: 48px;
				line-height: 48px;
				box-sizing: border-box;
				padding:0 25px ;
				text-align: right;
				font-size: 16px;
				position: relative;
				@extend %user-select-none;
				&:after{
					height: 32px;
					width:1px;
					display: block;
					content: "";
					position: absolute;
					top:8px;
					right:-1px;
					background:#e5e5e5;
				}
			}
		}
	}
	.slide-btn{
		width:18px;
		height: 32px;
		display: block;
		position: absolute;
		top:-11px;
		z-index: 3;
		>.btn{
			width:100%;
			height: 100%;
			background: #fff;
			display: block;
			cursor: pointer;
			box-sizing: border-box;
			border: 1px solid #d9d9d9;
			@include transition();
			&:hover{
				background: #f8f8f8;
			}
			&:active{
				background: #d9d9d9;
				&:before{
					background: #fff;
				}
				span{
					&:after{
						background: #fff;
					}
					&:before{
						background: #fff;
					}
				}
			}
			span{
				width:100%;
				height: 100%;
				position: absolute;
				display: block;
				top:0;
				left:0;
				&:after{
					width:1px;
					height: 18px;
					background: #d9d9d9;
					display: block;
					content: "";
					position: absolute;
					right:calc(50% - 3px);
					top:7px;
					@include transition();
				}
				&:before{
					width:1px;
					height: 18px;
					background: #d9d9d9;
					display: block;
					content: "";
					position: absolute;
					right:calc(50% + 3px);
					top:7px;
					@include transition();
				}
			}
			&:before{
				width:1px;
				height: 18px;
				background: #d9d9d9;
				display: block;
				content: "";
				position: absolute;
				right:calc(50%);
				top:7px;
				@include transition();
			}
		}
	}
	.slide-info{
		width:100%;
		height: 100%;
		position: absolute;
		left:0;
		top:0;
		z-index: 1;
		.start{
			position: absolute;
			left:0;
			top:100%;
			line-height: 36px;
			color:#666;
			@include translateX(-50%);
			padding-top:5px;
			font-size: 12px;
			&:after{
				width: 0;
				height: 0;
				position: absolute;
				content:"";
				border-bottom:4px solid #00c4e3;
				border-left: 3px solid transparent;
				border-right: 3px solid transparent;
				top:6px;
				left:calc(50% - 2px);
			}
		}
		.end{
			position: absolute;
			right:0;
			top:100%;
			line-height: 36px;
			color:#666;
			@include translateX(50%);
			padding-top:5px;
			font-size: 12px;
			&:after{
				width: 0;
				height: 0;
				position: absolute;
				content:"";
				border-bottom:4px solid #00c4e3;
				border-left: 3px solid transparent;
				border-right: 3px solid transparent;
				top:6px;
				left:calc(50% - 2px);
			}
		}
	}
}

.pn-num-picker{
	width:140px;
	float: left;
	height: 26px;
	box-sizing: border-box;
	position: relative;
	margin-top:2px;
	.pn-label{
		position: absolute;
		top: 0;
		left: 140px;
		right:100%;
		display: block;
		line-height: 26px;
		width:65px;
		font-weight: bold;
	}
	>label{
		width:140px;
		height: 100%;
		display: block;
		box-sizing: border-box;
		text-align: left;
		position: relative;
		float: left;
		z-index: 2;
		>span{
			position: absolute;
			left:calc(100% + 10px);
			height: 26px;
			line-height: 26px;
			color:#666;
			top:0;
		}
		>input{
			position: relative;
			width:100%;
			padding:0 15px 0 15px;
			border:1px solid #00c4e3;
			font-size: 12px;
			color:#666;
			height: 100%;
			box-sizing: border-box;
			text-align: right;
			outline: none;
			background: #d5e1e7;
		}
	}
	.controls{
		width:60px;
		float: left;
		height: 48px;
		position: relative;
		border:1px solid #e5e5e5;
		z-index: 1;
		left:-1px;
		top:0;
		box-sizing: border-box;
		.con-btn{
			position: absolute;
			width:100%;
			height: 24px;
			line-height: 24px;
			text-align: center;
			background: #fafafa;
			left:0;
			color:#8d8d8d;
			cursor: pointer;
			border-bottom:1px solid #e5e5e5;
			box-sizing: border-box;
			padding-left:2px;
			font-size: 16px;
			@include transition();
			&:hover{
				background: #eee;
			}
			&:active{
				box-shadow: inset 0 2px 3px rgba(#fff,1);
			}
			&.plus{
				top:0;
				box-shadow: inset 0 0 3px rgba(#fff,1);
				z-index: 2;
			}
			&.minus{
				bottom:-1px;
				z-index: 1;
				box-shadow:inset  0 0 3px rgba(#fff,1);
				line-height: 26px;
			}
		}
	}
	.error{
		position: absolute;
		top:100%;
		left:0;
		width:100%;
		padding:5px 0;
		line-height: 18px;
		font-size: 12px;
		background: lighten(#ed4821,15%);
		color:#fff;
		@include opacity(0);
		display: none;
		@extend %user-select-none;
		&.active{
			@include opacity(1);
			display: block;
		}
	}
}