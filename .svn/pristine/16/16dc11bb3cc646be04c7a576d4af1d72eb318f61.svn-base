			@include transitions;
			margin-bottom: 25px;
			&:hover{
				transform: translateX(25px);
   				-webkit-transform: translateX(25px);
   				-moz-transform: translateX(25px);
   				-ms-transform: translateX(25px);
			}
			&:hover .iconfont,&:hover .qq_go{
				color: #04afca;
			}
		}
		>.qq_img{
			display: inline-block;
			width: 25px;
			height: 25px;
			background: url(../img/login/login_ico.png) no-repeat -1px -1px;
		}
		>.qq_go{
			position: relative;
			top: -7px;
			font-size: 13px;
			color: #222222;
			@include transitions;
		}
		>.iconfont{
			position: relative;
			top: -9px;
			color: #222222;
			font-size:9px;
			@include transitions;
		}
	}
	.login_win{
		margin: 40px auto 40px auto;
		text-align: center;
		color: #222222;
		font-size: 16px;
		font-weight: 700;
		text-align: center;
	}
	.login_pass{
		font-size: 12px;
		margin-bottom: 18px;
		opacity: 0;
		@include transitions;
	}
	.login_pass_act{
		opacity: 1;
	}
}
