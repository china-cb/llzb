@charset "utf-8";
@import"core/setting";
@import"core/css3";
@import"core/_mixin";
%clearfixs{
  display:block;clear:both;content:"";visibility:hidden;height:0
}
@mixin transitions ($value: 0.6s){
     transition: all $value;
    -webkit-transition: all $value; 
    -moz-transition: all $value;
    -o-transition: all $value;
}
@mixin translateY($translateY:10px) {
    transform: translateY($translateY);
    -webkit-transform: translateY($translateY);
    -moz-transform: translateY($translateY);
    -ms-transform: translateY($translateY);
}
@mixin translateX($translateX:10px) {
    transform: translateX($translateX);
    -webkit-transform: translateX($translateX);
    -moz-transform: translateX($translateX);
    -ms-transform: translateX($translateX);
}
@mixin scales($scale1s:1,$scale2s:1) {
  transform: scale($scale1s,$scale2s);
    -webkit-transform: scale($scale1s,$scale2s);
    -moz-transform: scale($scale1s,$scale2s);
    -ms-transform: scale($scale1s,$scale2s);
}
@mixin rotates($rotates:45deg) {
  transform: rotate($rotates);
    -webkit-transform: rotate($rotates);
    -moz-transform: rotate($rotates);
    -ms-transform: rotate($rotates);
}
// 改变 input 输入框 placeholder 颜色
@mixin placeholders($colors:red) {
  &::-webkit-input-placeholder { /* WebKit browsers */ 
      color: $colors; 
  } 
  &:-moz-placeholder { /* Mozilla Firefox 4 to 18 */ 
      color: $colors; 
  } 
  &::-moz-placeholder { /* Mozilla Firefox 19+ */ 
      color: $colors; 
  }
  &:-ms-input-placeholder { /* Internet Explorer 10+ */ 
      color: $colors; 
  }  
}
