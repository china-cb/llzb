.pn-num-picker > label > span { position: absolute; left: calc(100% + 10px); height: 26px; line-height: 26px; color: #666; top: 0; }
.pn-num-picker > label > input { position: relative; width: 100%; padding: 0 15px 0 15px; border: 1px solid #00c4e3; font-size: 12px; color: #666; height: 100%; box-sizing: border-box; text-align: right; outline: none; background: #d5e1e7; }
.pn-num-picker .controls { width: 60px; float: left; height: 48px; position: relative; border: 1px solid #e5e5e5; z-index: 1; left: -1px; top: 0; box-sizing: border-box; }
.pn-num-picker .controls .con-btn { position: absolute; width: 100%; height: 24px; line-height: 24px; text-align: center; background: #fafafa; left: 0; color: #8d8d8d; cursor: pointer; border-bottom: 1px solid #e5e5e5; box-sizing: border-box; padding-left: 2px; font-size: 16px; -webkit-transition: all 0.3s ease; -ms-transition: all 0.3s ease; -moz-transition: all 0.3s ease; transition: all 0.3s ease; }
.pn-num-picker .controls .con-btn:hover { background: #eee; }
.pn-num-picker .controls .con-btn:active { box-shadow: inset 0 2px 3px white; }
.pn-num-picker .controls .con-btn.plus { top: 0; box-shadow: inset 0 0 3px white; z-index: 2; }
.pn-num-picker .controls .con-btn.minus { bottom: -1px; z-index: 1; box-shadow: inset 0 0 3px white; line-height: 26px; }
.pn-num-picker .error { position: absolute; top: 100%; left: 0; width: 100%; padding: 5px 0; line-height: 18px; font-size: 12px; background: #f38268; color: #fff; opacity: 0; filter: alpha(opacity=0); display: none; }
.pn-num-picker .error.active { opacity: 1; filter: alpha(opacity=100); display: block; }
