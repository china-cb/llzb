    			>.imgs{
    				display: block;
    				float: left;
    				width: 70px;
    				height: 100%;
    				margin-right: 8px;
    				overflow: hidden;
    				&:after{@extend %clearfix;}
    				>img{
    					width: 100%;
    					height: 100%;
    					@include transition;
    				}
    			}
    			>.img_h1{
    				position: relative;
    				top: -3px;
    				float: left;
    				width: 140px;
    				font-size: 14px;
    				color: #222222;
    				overflow: hidden;
					text-overflow: ellipsis;
					white-space: nowrap;
    			}
    			>.img_h2{
