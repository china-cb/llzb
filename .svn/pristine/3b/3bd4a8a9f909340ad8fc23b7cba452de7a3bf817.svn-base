.banner{
	position: relative;
	width: 100%;
	height: 640px;
	background: url("../img/experience/banner.jpg") no-repeat center center;
	background-size: cover;
	.main{
		margin: 0 auto;
		width: 1200px;
		height: 100%;
		.main_h1{
			padding-top: 30px;
			font-weight: 700;
			font-size: 31px;
			color: #222222
		}
		.main_h2{
			margin-bottom: 30px;
			color: #00c1df;
			font-size: 14px;
		    font-weight: 700;
		}
		>.main_list{
			width: 100%;
			height: 350px;
			@extend %clearfix;
			>li{
				position: relative;
				cursor: pointer;
				border-radius: 3px;
				margin-left: 17px;
				float: left;
				width: 215px;
				font-weight: 700;
				height: 100%;
				background-color: #fff;
				box-shadow: 0 0 32px rgba(#00c1df,0.35);
				@extend %clearfix;
				&:first-child{margin-left: 29px;}
				@include transition;
				>span{
					position: relative;
					margin: 0 auto;
				    display: block;
				    width: 155px;
				    text-align: left;
				}
				>a{
					margin: 0 auto;
					display: block;
					width: 112px;
					height: 33px;
					line-height: 33px;
					color: #fff;
					border-radius: 2px;
					font-size: 18px;
					background-color: #00c1df;
					box-shadow: 0 0 32px rgba(#00c1df,0.2);
					@include transition;
					&:hover{background-color: #00b0cc;box-shadow: 0 0 32px rgba(#00b0cc,0.2);}
				}
				.spa_img{
					width: 215px;
					height: 133px;
					border-radius: 3px;
					overflow: hidden;
					>img{
						width: 100%;
						height: 100%;
						@include transition;
					}
				}
				.spa_h1{
					height: 45px;
					line-height: 45px;
					color: #222222;
					font-size: 18px;
					>span{color: #999999;font-size: 14px;position: relative;top: -1px;margin-left: 20px;}
					>i{position: absolute;width: 25px;height: 3px;background-color: #00c1df;left: 0;bottom: 0}
				}
				.spa_h2{
					position: relative;
					margin: 13px auto 15px auto;
					height: 88px;
					color: #a0b1b4;
					font-size: 12px;
					>.spa_ll{
						position: absolute;
						top: 7px;
						float: left;
						display: block;
						width: 55px;
						height: 60px;
						text-align: center;
						color: #222;
						line-height: 100px;
						background: url("../img/favicon.png") no-repeat center top;
						background-size: 37px 37px;
					}
					>.spa_rw{
						float: right;
						display: block;
						width: 80px;
						height: 80px;
					}
				}
				&:hover{box-shadow: 0 8px 32px rgba(#00c1df,0.8);@include translateY(-5px);}
				&:hover .spa_img>img{@include scale(1.05);}
			}
		}
	}
}
.interact{
	width: 100%;
	height: 430px;
	background-color: #fff;
	>.main_h2{color: #999999}
	>.interact_main{
		margin: 50px auto 50px auto;
		width: 960px;
		height: 110px;
		&:after{@extend %clearfix;}
		li{
			position: relative;
			float:left;
			width: 290px;
			height: 100%;
			margin-right: 45px;
			&:after{@extend %clearfix;}
			>.ico_left{
				position: relative;
				display: block;
				float: left;
				width: 110px;
				height: 110px;
				border-radius: 50%;
				background-color: #6f7cc0;
				&:after{@extend %clearfix;}
				// @include rotate();
				>span{
					display: block;
					position: absolute;
					top: 50%;
					left: 50%;
					margin-left: -28px;
					margin-top: -28px;
					width: 56px;
					height: 56px;
					border-radius: 50%;
					line-height: 56px;
					background-color: #5f6cb0;
					&:after{
						color: #fff;
						content: " ";
						@extend %iconfont;
		    			content:"\e63a";
		    			font-size: 35px;
						}
				}
			}
			&:nth-child(2){
				>.ico_left{
					&:first-child{
						width: 110px;
						height: 110px;
						border-radius: 50%;
						background-color: #5fb48d;
						>span{
							background-color: #509877;
							&:after{
				    			content:"\e651";
							}
						}
					}
				}
			}
			&:nth-child(3){
				margin-right: 0;
				>.ico_left{
					&:first-child{
						width: 110px;
						height: 110px;
						border-radius: 50%;
						background-color: #77abf2;
						>span{
							background-color: #4b6bbd;
							&:after{
				    			content:"\e653";
							}
						}
					}
				}
			}
			.content_right{
				text-align: left;
				float: right;
				display: block;
				width: 160px;
				height: 100%;
				&:after{@extend %clearfix;}
				>span{
					display: block;
					width: 100%;
					&:first-child{
						color: #222;
						font-size: 16px;
						font-weight: 700;
						margin:20px 0 15px 0;
					}
					&:nth-child(2){
						color: #a4cad0;
						font-size: 12px;
						font-weight: 700;
					}
				}
			}
		}
	}
	>a{
		display: block;
		margin: 0 auto;
		width: 110px;
		height: 30px;
		line-height: 30px;
		color: #a0b1b4;
		font-weight: 700;
		border: 1px solid #a4cad0;
		@include transition;
		&:hover{background-color: #a4cad0;color: #fff;box-shadow: 0 0 15px #a4cad0;@include translateX(3px);}
	}
}
.cloud{
	position: relative;
	width: 100%;
	height: 595px;
	background: linear-gradient(to right,#37d2ae, #4d94e0);
	>.cloud_main{
		position: relative;
		top: 5px;
		margin: 0 auto;
		width: 1200px;
		height: 573px;
		color: #fff;
		background: url("../img/experience/media.png") no-repeat center center;
		>ul{
			position: absolute;
			>li{
				font-size: 24px;
				margin-bottom: 5px;
				&:nth-child(2){
					font-size: 14px;
				}
			}
			&:nth-child(odd){text-align: right;}
			&:nth-child(even){text-align: left;}
			&:first-child{
				top: 55px;
   				left: 30px;
			}
			&:nth-child(3){
				top: 312px;
   				left: 0;
			}
			&:nth-child(2){
				top: 90px;
   				right: 45px;
			}
			&:nth-child(4){
				top: 360px;
   				right: 55px;
			}
		}
		h1{font-size: 49px;position: relative;top: 410px}
		h2{font-size: 24px;position: relative;top: 395px}
	}
}
