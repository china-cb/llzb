%iconfont {
  font-family:"iconfont" !important;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  @extend %user-select-none;
}
html,body{
	background-color: #fff;
}
.banners_slide{
	margin: 0 auto;
	width: 100%;
	height: 610px;
}
.swiper-container {
        width: 100%;
        height: 100%;
        margin-left: auto;
        margin-right: auto;
    }
    .swiper-slide {
        text-align: center;
        font-size: 18px;
        background: #fff;
        /* Center slide text vertically */
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
		background: url("../img/applications/ico_01.jpg") no-repeat center center;
		&:nth-child(2){background-image: url("../img/applications/ico_02.jpg")}
		&:nth-child(3){background-image: url("../img/applications/ico_03.jpg")}
		&:nth-child(4){background-image: url("../img/applications/ico_04.jpg")}
		background-size: cover;
		.slide_main{
			margin: 0 auto;
			width: 1135px;
			height: 100%;
			color: #fff;
			text-align: left;
			padding-left: 65px;
			h1{
				margin: 145px auto 15px auto;
				font-size: 52px;
			}
			h2{
				width: 680px;
				font-size: 12px;
			}
			a{
				display: inline-block;
				margin: 40px auto 20px auto;
				letter-spacing: 1px;
				font-size: 18px;
				color: #00c1df;
				@include transition();
				&:hover{@include translateX(5px);color: #00bad7;}
			}
			span{
				cursor: pointer;
				display: block;
				width: 170px;
				height: 50px;
				text-align: center;
				line-height: 50px;
				font-size: 18px;
				border: 2px solid #fff;
				@include transition();
				letter-spacing: 1px;
				&:hover{
					   box-shadow: 0 0 7px #fff;
					   text-shadow: 0 0 1px #fff;
				}
			}
		}
    }
    .swiper-pagination-bullet{
    	margin: 10px 0 !important;
    	position: relative;
        width: 205px;
        height: 122px;
        border:3px solid transparent;
        border-radius: 1px; 
        opacity: 1;
        @include transition;
        text-align: center;
        background: #000;

        // background: url("../img/applications/applications_ico.png") no-repeat -18px -12px;
        // &:nth-child(2){background-position: -18px -161px;}
        // &:nth-child(3){background-position: -18px -304px;}
        // &:nth-child(4){background-position: -18px -444px;}
        // background-color: rgba(#000,0.5);
    }
    .swiper-pagination-bullet-active{
        border-radius: 5px; 
        background: #000;
        border: 3px solid #00c1df;
         overflow: hidden;
         img{opacity: 1;}
    }
    .swiper-pagination{
		left: 50%;
		margin-left: 400px;
    }
    .bullet_title{
    	position: relative;
    	z-index: 2;
    	margin: 12px auto 0 auto;
    	color: #fff;
    	font-size: 18px;
    }
    .bullet_contet{
    	position: relative;
    	z-index: 2;
    	display: block;
    	margin: 0 auto;
    	width: 160px;
    	font-size: 12px;
    	color: #a4a4a4;
    }
    .blacks{
    	position: absolute;
	    top: -3px;
	    left: -2px;
	    z-index: 1;
	    width: 103%;
	    height: 105%;
	    opacity: 0.5;
    }
