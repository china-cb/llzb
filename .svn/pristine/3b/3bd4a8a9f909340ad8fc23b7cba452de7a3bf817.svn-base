.applications_inner{
	margin: 0 auto;
	padding-top: 20px;
	width: 920px;
    height: auto;
    &:after{@extend %clearfix;}
    >section{
    	float: left;
    	padding: 20px 35px;
    	width: 580px;
    	height: auto;
    	background-color: #fff;
    	&:after{@extend %clearfix;}
    	>h1{
    		padding: 0 15px;
    		width: 190px;
