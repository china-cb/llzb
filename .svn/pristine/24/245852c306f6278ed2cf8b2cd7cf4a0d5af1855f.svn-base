.guide .logo_location .login_agreement > i { position: absolute; width: 10px; height: 10px; background-color: #00c1df; z-index: 1; top: 5px; left: 5px; border-radius: 50%; transition: all 0.3s; -webkit-transition: all 0.3s; /* Safari */ -moz-transition: all 0.3s; -o-transition: all 0.3s; transform: scale(0, 0); -webkit-transform: scale(0, 0); -moz-transform: scale(0, 0); -ms-transform: scale(0, 0); }
